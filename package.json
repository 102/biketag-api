{
  "name": "biketag-api",
  "version": "0.0.1",
  "description": "The Javascript client API for BikeTag Games",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keneucker/biketag-api.git"
  },
  "author": "Ken Eucker",
  "license": "CC-BY-SA-4.0",
  "bugs": {
    "url": "https://github.com/keneucker/biketag-api/issues"
  },
  "homepage": "https://github.com/keneucker/biketag-api#readme",
  "scripts": {
    "dev": "npx eleventy --serve",
    "test": "npx jest",
    "test:ci": "npx jest --ci --coverage --runInBand",
    "build": "npx tsc",
    "typecheck": "npx tsc --noEmit",
    "clean": "rm -rf lib && rm -rf _site",
    "prepare": "npx husky install",
    "lint": "npx eslint . && prettier --check . && npm run typecheck",
    "commit": "npx cz",
    "postinstall": "npm run test",
    "convert": "npx typeconv -f ts -t oapi -o openapi 'src/**/*.ts'"
  },
  "dependencies": {
    "@sanity/client": "^2.7.1",
    "form-data": "^4.0.0",
    "got": "^11.8.1",
    "imgur": "^2.0.0-next.5",
    "typeconv": "^1.4.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@types/jest": "^26.0.22",
    "@types/mock-fs": "^4.13.0",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "mock-fs": "^4.13.0",
    "msw": "^0.28.1",
    "prettier": "^2.2.1",
    "semantic-release": "^17.4.1",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  },
  "lint-staged": {
    "*.ts": [
      "eslint . --fix",
      "jest --bail --findRelatedTests"
    ],
    "*.{js,css,md,yml,yaml,json}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
